{
  "name": "chatdnd",
  "version": "1.0.0",
  "description": "A dnd chatbot",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:client": "node esbuild-client.config.mjs",
    "build:server": "node esbuild-server.config.mjs",
    "start": "NODE_ENV=production node dist/server/index.js",
    "dev": "NODE_ENV=development npm-run-all --parallel dev:*",
    "dev:client": "node esbuild-client.config.mjs --dev",
    "dev:server": "concurrently \"node esbuild-server.config.mjs --dev\" \"NODE_ENV=dev nodemon dist/server/index.js\"",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonbw/ChatDnD.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonbw/ChatDnD/issues"
  },
  "homepage": "https://github.com/simonbw/ChatDnD#readme",
  "nodemonConfig": {
    "enable-source-maps": true,
    "watch": [
      "dist/server"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.5",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "elevenlabs-api": "^1.0.6",
    "esbuild": "^0.17.15",
    "esbuild-serve": "^1.0.1",
    "esbuild-style-plugin": "^1.6.1",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "nodemon": "^2.0.22",
    "open-simplex-noise": "^2.5.0",
    "openai": "^3.2.1",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "sharp": "^0.32.0",
    "tailwindcss": "^3.3.1",
    "tailwindcss-opentype": "^1.1.0",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "@types/postcss-import": "^14.0.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7"
  }
}
